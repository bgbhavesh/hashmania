{
  "format": "unipackage-pre1",
  "summary": "Allows handlebars templates to be defined on the server in .handlebars files",
  "slices": [
    {
      "name": "main",
      "arch": "browser",
      "path": "browser.json"
    },
    {
      "name": "main",
      "arch": "os",
      "path": "os.json"
    },
    {
      "name": "tests",
      "arch": "browser",
      "path": "tests.browser.json"
    },
    {
      "name": "tests",
      "arch": "os",
      "path": "tests.os.json"
    }
  ],
  "defaultSlices": {
    "browser": [
      "main"
    ],
    "os": [
      "main"
    ]
  },
  "testSlices": {
    "browser": [
      "tests"
    ],
    "os": [
      "tests"
    ]
  },
  "plugins": [
    {
      "name": "compileServerHandlebarsTemplates",
      "arch": "os",
      "path": "plugin.compileServerHandlebarsTemplates.os/program.json"
    }
  ]
}